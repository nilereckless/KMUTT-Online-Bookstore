<style>
    #detail{
            font-size: 20px;
            border-radius: 25px;
            background: seashell;
            padding: 20px;
            width: 100vw;
            height: 250px;
    }
    #amount{
        font-size: 50px;
        color: #de0606;
    }
    #contact{
        padding-top: 10px;
        font-size: 30px;
    }
    #info{
        padding-left: 40px;
    }
</style>
<body>
    <h1 align="center">หลักฐานการชำระเงิน
    </h1><br>
    <div class="container">
        <div class="row mx-0 justify-content-xl center">
            <div class="col-lg-6">
                <img src="https://kmuttonlinebookstore.me/<%=data[0].slip%>" height="500px" width="auto">
            </div>
            <div class="col-lg-6" id="detail">
                <div align="center" id="amount"><b><%=data[0].amount%> ฿</b></div>
                <div id="contact"><b>Contact</b></div>
                <div id="info"><%=data[0].fullName%><br>
                
                    Email : <%=data[0].eMail%><br>
                    Tel : <%=data[0].phone%></div>
                
                </div>
        </div>
    </div>
    
</body>
